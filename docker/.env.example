# Docker build secrets - copy to .env and fill in values
# DO NOT commit .env to git

# Password for dev user in r-dev container (required)
DEV_PASSWORD=

# GitHub Personal Access Token for private package installation
GITHUB_PAT=

# RStudio Server password (optional, defaults to 'rstudio')
RSTUDIO_PASSWORD=

# Local username for reverse SSH tunnel (optional)
LOCAL_USER=
